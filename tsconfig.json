{
  "compilerOptions": {
    "outDir": "./lib/",
    "module": "commonjs",
    "declaration": true,
    "target": "es5",
    "jsx": "react-jsx",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "forceConsistentCasingInFileNames": true,
    "suppressImplicitAnyIndexErrors": true,
    "strict": true
  }
}
